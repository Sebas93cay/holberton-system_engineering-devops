#!/usr/bin/env basho
# This script prints numbers from 1 to 20 with some bad luck numbers
i=1
while [ $i -lt 21 ]
do
    case $i in
    4)
        echo "bad luck from China"
        ;;
    9)
        echo "bad luck from Japan"
        ;;
    17)
        echo "bad luck from Italy"
        ;;
    *)
        echo "$i"
        ;;
    esac
    i=$((i+1))
done
